{
  "manifest_version": 3,
  "name": "Tracker Detector",
  "version": "1.0",
  "description": "Lists all trackers on the current page",
  "permissions": [
    "webRequest",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}